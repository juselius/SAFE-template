{
    "$schema": "http://json.schemastore.org/template",
    "author": "Tomasz Heimowski",
    "classifications": [
        "Web",
        "Saturn",
        "Azure",
        "Fable",
        "Elmish"
    ],
    "name": "SAFE-Stack Web App v1.21.0",
    "tags": {
        "language": "F#",
        "type": "project"
    },
    "preferNameDirectory": true,
    "identity": "SAFE.Template",
    "shortName": "SAFE",
    "sourceName": "SAFE.Template",
    "symbols": {
        "createdate": {
            "type": "generated",
            "generator": "now",
            "parameters": {
              "format": "yyyy-MM-dd"
            },
            "replaces":"1970-01-01"
        },
        "server": {
            "type": "parameter",
            "dataType": "choice",
            "defaultValue": "giraffe",
            "choices": [
                {
                    "choice": "suave",
                    "description": "lightweight, non-blocking web server (standalone)"
                },
                {
                    "choice": "giraffe",
                    "description": "a functional micro web framework based on ASP.NET Core"
                },
                {
                    "choice": "saturn",
                    "description": "web development framework which implements the server-side MVC pattern"
                }
            ]
        },
        "layout": {
            "type": "parameter",
            "dataType": "choice",
            "defaultValue": "fulma-basic",
            "choices": [
                {
                    "choice": "none",
                    "description": "don't add any CSS framework"
                },
                {
                    "choice": "fulma-basic",
                    "description": "add Fulma basic template"
                },
                {
                    "choice": "fulma-admin",
                    "description": "add Fulma with 'Admin' Bulma template from https://dansup.github.io/bulma-templates/"
                },
                {
                    "choice": "fulma-cover",
                    "description": "add Fulma with 'Cover' Bulma template from https://dansup.github.io/bulma-templates/"
                },
                {
                    "choice": "fulma-hero",
                    "description": "add Fulma with 'Hero' Bulma template from https://dansup.github.io/bulma-templates/"
                },
                {
                    "choice": "fulma-landing",
                    "description": "add Fulma with 'Landing' Bulma template from https://dansup.github.io/bulma-templates/"
                },
                {
                    "choice": "fulma-login",
                    "description": "add Fulma with 'Login' Bulma template from https://dansup.github.io/bulma-templates/"
                }
            ]
        },
        "communication": {
            "type": "parameter",
            "dataType": "choice",
            "defaultValue": "none",
            "choices": [
                {
                    "choice": "none",
                    "description": "don't add any additional libraries for client-server communication"
                },
                {
                    "choice": "remoting",
                    "description": "add Fable.Remoting (https://zaid-ajaj.github.io/Fable.Remoting/) to server and client"
                },
                {
                    "choice": "bridge",
                    "description": "add Elmish.Bridge (https://github.com/Nhowka/Elmish.Bridge) to server and client"
                }
            ]
        },
        "remoting": {
            "type": "computed",
            "value": "(communication == \"remoting\")"
        },
        "bridge": {
            "type": "computed",
            "value": "(communication == \"bridge\")"
        },
        "pattern": {
            "type": "parameter",
            "dataType": "choice",
            "defaultValue": "default",
            "choices": [
                {
                    "choice": "default",
                    "description": "use standard Elmish architecture with Commands"
                },
                {
                    "choice": "streams",
                    "description": "use simple Elmish architecture without Commands + Elmish.Streams (https://fablereaction.readthedocs.io/en/latest/fable.reaction) for reactive pattern"
                }
            ]
        },
        "streams": {
            "type": "computed",
            "value": "(pattern == \"streams\")"
        },
        "js-deps": {
            "type": "parameter",
            "dataType": "choice",
            "defaultValue": "yarn",
            "choices": [
                {
                    "choice": "npm",
                    "description": "uses NPM for JS package management"
                },
                {
                    "choice": "yarn",
                    "description": "uses Yarn for JS package management"
                }
            ]
        },
        "deploy": {
            "type": "parameter",
            "dataType": "choice",
            "defaultValue": "docker",
            "choices": [
                {
                    "choice": "none",
                    "description": "don't add FAKE targets to deploy"
                },
                {
                    "choice": "azure",
                    "description": "additional FAKE targets to deploy to Azure App Service"
                },
                {
                    "choice": "docker",
                    "description": "additional FAKE targets to bundle and build Docker image"
                },
                {
                    "choice": "gcp-appengine",
                    "description": "additional FAKE targets to deploy to Google Cloud AppEngine"
                },
                {
                    "choice": "gcp-kubernetes",
                    "description": "additional FAKE targets to publish to gcp container registry and deploy to google kubernetes engine"
                },
                {
                    "choice": "iis",
                    "description": "FAKE targets and client paths normalization to easily publish the application to IIS"
                },
                {
                    "choice": "heroku",
                    "description": "additional FAKE targets to deploy on Heroku"
                }
            ]
        }
    },
